<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>javaWebApplication</display-name>

	<servlet>
		<servlet-name>RegisterUser</servlet-name>
		<servlet-class>javaWebApplication.Controller.RegisterUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterUser</servlet-name>
		<url-pattern>/RegisterUser</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginUser</servlet-name>
		<servlet-class>javaWebApplication.Controller.LoginUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginUser</servlet-name>
		<url-pattern>/LoginUser</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AgendamentoUser</servlet-name>
		<servlet-class>javaWebApplication.Controller.AgendamentoUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AgendamentoUser</servlet-name>
		<url-pattern>/AgendamentoUser</url-pattern>
	</servlet-mapping>





	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
